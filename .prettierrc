{
    "trailingComma": "all",
    "tabWidth": 4,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "singleAttributePerLine": true,
    "arrowParens": "always",
    "plugins": [
        "prettier-plugin-tailwindcss",
        "prettier-plugin-classnames",
        "prettier-plugin-merge"
    ]
}
